
\RequirePackage{csquotes}
\PassOptionsToPackage{%
  citestyle=numeric,%
  maxnames=20,%
  defernumbers=true
}{biblatex}
\AtEndPreamble{
  \RequirePackage{biblatex}}

\AfterPackage{biblatex}{
  \ifthenelse{\boolean{@swa@german}}{
    \ExecuteBibliographyOptions{
      url=false,%
      language=ngerman,%
      urldate=iso8601%
    }
  }{
    \ExecuteBibliographyOptions{%
      url=false,%
      language=american,%
      urldate=iso8601%
    }
  }
}